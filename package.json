  "main": "index.js",
  "scripts": {
    "cy:test": "cypress run",
    
    "cy:test:mobile:chrome:headed": "cypress run --browser chrome --headed --config-file cypress.mobile.config.js",
    "cy:test:mobile:chrome:headless": "cypress run --browser chrome --headless --config-file cypress.mobile.config.js",
    "cy:test:mobile:edge:headed": "cypress run --browser edge --headed --config-file cypress.mobile.config.js",
    "cy:test:mobile:edge:headless": "cypress run --browser edge --headless --config-file cypress.mobile.config.js",
    
    "cy:test:note:chrome:headed": "cypress run --browser chrome --headed --config-file cypress.note.config.js",
    "cy:test:note:chrome:headless": "cypress run --browser chrome --headless --config-file cypress.note.config.js",
    "cy:test:note:edge:headed": "cypress run --browser edge --headed --config-file cypress.note.config.js",
    "cy:test:note:edge:headless": "cypress run --browser edge --headless --config-file cypress.note.config.js"
  },
